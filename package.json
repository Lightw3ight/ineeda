{
  "name": "ineeda",
  "version": "0.11.3",
  "description": "Create stub objects from TypeScript interfaces!",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": "phenomnomnominal/ineeda",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "lint": "npm run lint:src && npm run lint:test",
    "lint:src": "tslint -c tslint.json 'src/**/*.ts'",
    "lint:test": "tslint -c tslint.json 'test/**/*.ts'",
    "lint:fix": "npm run lint:src:fix && npm run lint:test",
    "lint:src:fix": "tslint -c tslint.json 'src/**/*.ts' --fix",
    "lint:test:fix": "tslint -c tslint.json 'test/**/*.ts' --fix",
    "test:jest": "jest",
    "build": "npm run compile && npm run lint && npm run test:jest",
    "build:ci": "npm run compile && npm run lint && npm run test:jest",
    "prepublishOnly": "rm -rf dist && npm run build:ci"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.11",
    "@types/jasmine": "^5.1.0",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/sinon": "^17.0.2",
    "@types/sinon-chai": "^3.2.11",
    "bluebird": "^3.7.2",
    "chai": "^4.3.10",
    "dedent": "^0.7.0",
    "jest": "^29.7.0",
    "rxjs": "^7.8.1",
    "sinon": "^17.0.1",
    "sinon-chai": "^3.7.0",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tslint": "^6.1.3",
    "typescript": "^5.3.3"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "testRegex": "/test/.*\\.spec\\.ts$"
  }
}
