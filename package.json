{
  "name": "ineeda",
  "version": "0.2.0-beta.5",
  "description": "Create stub objects from TypeScript interfaces!",
  "main": "dist/src/index.js",
  "repository": "phenomnomnominal/ineeda",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "test:jest": "jest",
    "test:karma": "karma start karma.conf.js",
    "test:mocha": "mocha --opts ./.mocharc 'dist/**/*.spec.js'",
    "build": "npm run compile && npm run test:jest && npm run test:mocha && npm run test:karma"
  },
  "dependencies": {
    "source-map-support": "0.4.8",
    "typescript": "2.0.10"
  },
  "devDependencies": {
    "@types/chai": "3.4.34",
    "@types/jasmine": "2.5.40",
    "@types/node": "7.0.0",
    "@types/source-map-support": "0.2.28",
    "chai": "3.5.0",
    "jasmine": "2.5.3",
    "jest": "18.1.0",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-ineeda": "0.1.0-beta.2",
    "karma-jasmine": "1.1.0",
    "karma-typescript": "2.1.6",
    "mocha": "3.2.0",
    "ts-jest": "18.0.1"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.spec\\.ts$"
  }
}
