{
  "name": "ineeda",
  "version": "0.11.0-beta.2",
  "description": "Create stub objects from TypeScript interfaces!",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "repository": "phenomnomnominal/ineeda",
  "author": "Craig Spence <craigspence0@gmail.com>",
  "license": "MIT",
  "scripts": {
    "compile": "tsc",
    "test:jest": "jest",
    "test:karma": "karma start karma.conf.js",
    "test:mocha": "nyc mocha './test/*.spec.ts'",
    "build": "npm run compile && npm run test:jest && npm run test:karma && npm run test:mocha",
    "prepublish": "rm -rf dist && npm run build"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bluebird": "3.0.37",
    "@types/chai": "3.4.34",
    "@types/jasmine": "2.5.40",
    "@types/node": "7.0.0",
    "@types/sinon": "1.16.35",
    "@types/sinon-chai": "2.7.27",
    "bluebird": "3.4.7",
    "chai": "3.5.0",
    "dedent": "^0.7.0",
    "jasmine": "2.5.3",
    "jest": "18.1.0",
    "karma": "1.3.0",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-jasmine": "1.1.0",
    "karma-safari-launcher": "^1.0.0",
    "karma-typescript": "2.1.6",
    "mocha": "3.2.0",
    "moment": "2.17.1",
    "nyc": "^11.0.2",
    "rxjs": "5.2.0",
    "sinon": "1.17.7",
    "sinon-chai": "2.8.0",
    "ts-jest": "18.0.1",
    "ts-node": "2.1.0",
    "typescript": "^2"
  },
  "jest": {
    "globals": {
      "__TS_CONFIG__": "tsconfig.json"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "\\.(ts)$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/test/.*\\.spec\\.ts$"
  },
  "nyc": {
    "include": [
      "**/*.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
